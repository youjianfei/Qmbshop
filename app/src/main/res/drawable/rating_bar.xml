<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 必须使用此id,drawable为 未选中 状态的图片 -->
    <item
        android:id="@+android:id/background"
        android:drawable="@mipmap/xingxinggray"/>
